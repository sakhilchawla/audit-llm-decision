{
  "name": "mcp-logging-server",
  "version": "1.0.0",
  "description": "Simple Model Control Plane Logging Server",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:test": "NODE_ENV=test ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "npm run test:setup-db && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "docker:build": "docker build -t mcp-logging-server .",
    "docker:run": "docker run -p 4000:4000 mcp-logging-server",
    "prepare": "husky install",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "test:setup-db": "ts-node scripts/setup-test-db.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.18.126",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  }
}

  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^16.18.126",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^8.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  }
